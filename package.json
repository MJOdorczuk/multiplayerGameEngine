    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^1.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.5",
    "nodemon-webpack-plugin": "^4.0.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.16.2",
